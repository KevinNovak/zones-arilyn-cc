{
	"name": "zones-arilyn-cc",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"start": "npm run start:preview",
		"start:dev": "npm run dev",
		"start:preview": "npm run build && npm run preview",
		"start:pm2": "npm run build && npm run pm2:start",
		"pm2:start": "pm2 start process.json",
		"pm2:stop": "pm2 stop process.json",
		"pm2:delete": "pm2 delete process.json"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/kit": "^2.33.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@types/leaflet": "^1.9.20",
		"@types/luxon": "^3.7.1",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"bootstrap": "^5.3.7",
		"leaflet": "^1.9.4",
		"luxon": "^3.7.1",
		"pm2": "^6.0.8"
	}
}
